{
  "name": "simple-crud-demo",
  "version": "0.1.1",
  "description": "A simple CRUD app, used as a demo for some NGINX tools.",
  "main": "index.js",
  "scripts": {
    "clean": "npm prune --production",
    "test": "tape test/test-*.js",
    "build": "echo \"no build for this site\"",
    "start": "node index.js --ports=$npm_package_config_ports --domain=$npm_package_config_domain",
    "isup": "tape validate/test-*.js"
  },
  "config": {
    "domain": "simplecrud.tobiaslabs.com",
    "ports": "[4000]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiaslabs/simple-crud-demo.git"
  },
  "keywords": [
    "demo",
    "example",
    "api",
    "crud"
  ],
  "author": "saibotsivad",
  "license": "VOL",
  "bugs": {
    "url": "https://github.com/tobiaslabs/simple-crud-demo/issues"
  },
  "homepage": "https://github.com/tobiaslabs/simple-crud-demo#readme",
  "dependencies": {
    "express": "^4.13.4",
    "minimist": "^1.2.0",
    "mustache": "^2.2.1",
    "psy": "^1.7.2",
    "request": "^2.72.0",
    "reserved-numbers-cli": "^0.2.0",
    "tape": "^4.5.1"
  }
}
